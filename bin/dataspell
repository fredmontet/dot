#!/bin/bash

# `dataspell` Start the DataSpell editor
# 
# Generated by JetBrains Toolbox 1.27.0.13477 at 2022-11-28T11:21:09.361120
#
# :param args: command arguments 

declare -a ideargs=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    ideargs+=("\"$o\"")
  else
    ideargs+=("$o")
  fi
done

open -na "~/Library/Application Support/JetBrains/Toolbox/apps/DataSpell/ch-0/222.4459.25/DataSpell.app/Contents/MacOS/dataspell" $wait --args "${ideargs[@]}"