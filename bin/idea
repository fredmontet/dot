#!/bin/bash
#Generated by JetBrains Toolbox 1.26.3.13353 at 2022-10-24T18:20:39.716483

declare -a ideargs=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    ideargs+=("\"$o\"")
  else
    ideargs+=("$o")
  fi
done

open -na "/Users/frederic.montet/Library/Application Support/JetBrains/Toolbox/apps/IDEA-U/ch-0/223.6160.11/IntelliJ IDEA 2022.3 EAP.app/Contents/MacOS/idea" $wait --args "${ideargs[@]}"