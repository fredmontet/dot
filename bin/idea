#!/bin/bash

# `idea` Start the Idea editor
# 
# Generated by JetBrains Toolbox 1.27.0.13477 at 2022-11-28T11:21:09.365328
#
# :param args: command arguments 

declare -a ideargs=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    ideargs+=("\"$o\"")
  else
    ideargs+=("$o")
  fi
done

open -na "/Users/fred.montet/Library/Application Support/JetBrains/Toolbox/apps/IDEA-U/ch-0/222.4459.24/IntelliJ IDEA.app/Contents/MacOS/idea" $wait --args "${ideargs[@]}"